{
  "api": {
    "services": [
      "HandlerService",
      "LoggerService",
      "StatsService"
    ],
    "tag": "api"
  },
  "dns": {
    "queryStrategy": "UseIPv4",
    "servers": [
	  #Family Friend DNS
      #"1.1.1.3",
      #"1.0.0.3",
      #"94.140.14.15",
      #"94.140.15.16"
	  
    ]
  },,
  "fakeDns": null,
  "inbounds": [
    {
      "listen": "127.0.0.1",
      "port": 62789,
      "protocol": "dokodemo-door",
      "settings": {
        "address": "127.0.0.1"
      },
      "sniffing": null,
      "streamSettings": null,
      "tag": "api"
    }
  ],
  "log": {
    "loglevel": "warning"
  },
  "outbounds": [
    {
      "protocol": "freedom",
      "settings": {}
    },
    {
      "protocol": "blackhole",
      "settings": {},
      "tag": "blocked"
    },
	{
      "protocol": "freedom",
      "settings": {
        "domainStrategy": "UseIPv4"
      },
      "tag": "IPv4"
    }
  ],
  "policy": {
    "levels": {
      "0": {
        "statsUserDownlink": true,
        "statsUserUplink": true
      }
    },
    "system": {
      "statsInboundDownlink": true,
      "statsInboundUplink": true
    }
  },
  "reverse": null,
  "routing": {
    "domainStrategy": "IPIfNonMatch",
    "rules": [
      {
        "inboundTag": [
          "api"
        ],
        "outboundTag": "api",
        "type": "field"
      },
	  {
        "ip": [
          "geoip:private"
        ],
        "outboundTag": "blocked",
        "type": "field"
      },
      {
        "ip": [
          "geoip:ir",
          "geoip:cn",
          "geoip:ru"
        ],
        "outboundTag": "blocked",
        "type": "field"
      },
      {
        "domain": [
          "regexp:.*\\.ir$",
          "ext:iran.dat:ir",
          "ext:iran.dat:other",
          "geosite:category-ir",
          "geosite:cn",
          "regexp:.*\\.cn$",
          "geosite:category-gov-ru",
          "regexp:.*\\.ru$"
        ],
        "outboundTag": "blocked",
        "type": "field"
      },
	  {
        "domain": [
          "geosite:category-ads-all",
          "ext:iran.dat:ads"
        ],
        "outboundTag": "blocked",
        "type": "field"
      },
	   {
        "domain": [
          "geosite:google",
          "geosite:netflix"
        ],
        "outboundTag": "IPv4",
        "type": "field"
      },
	  {
        "outboundTag": "blocked",
        "protocol": [
          "bittorrent"
        ],
        "type": "field"
      }
    ]
  },
  "stats": {},
  "transport": null
}